{
  "@context": "http://www.w3.org/ns/csvw",
  "schema:name": "Tomatoes",
  "schema:description": "Data about tomato varieties Sam has grown",
  "schema:creator": {
    "schema:name": "Sam",
    "schema:url": "http://sam.pikesley.org",
    "@type": "schema:Person"
  },
  "url": "tomatoes.csv",
  "tableSchema": {
    "columns": [
      {
        "titles": "common name",
        "schema:description": "Common name of tomato variety",
        "datatype": "string"
      },
      {
        "titles": "botanical name",
        "schema:description": "Technical name of tomato variety",
        "datatype": "string"
      },
      {
        "titles": "type",
        "name": "tomato_type",
        "schema:description": "Growing habit of tomato variety"
      }
    ],
    "foreignKeys": [
      {
        "columnReference": "tomato_type",
        "reference": {
          "resource": "tomato-types.csv",
          "columnReference": "type"
        }
      }
    ],
    "primaryKey": "common name"
  }
}
